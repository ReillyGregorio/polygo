{
  "entrypoint": "res/index.html",
  "shell": "res/src/my-app.html",
  "fragments": [
    "res/src/my-view1.html",
    "res/src/my-view2.html",
    "res/jsrc/my-view3.html",
    "res/src/my-view404.html"
  ],
  "sources": [
    "res/src/**/*",
    "res/images/**/*"
  ],
  "extraDependencies": [
    "res/manifest.json",
    "res/bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
